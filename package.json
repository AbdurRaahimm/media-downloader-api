{
    "name": "video-downloader-api",
    "version": "1.0.0",
    "description": "Social media video downloader API with rate limiting and versioning",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "start": "node --watch server.js",
        "dev": "nodemon server.js",
        "build": "curl -L https://github.com/yt-dlp/yt-dlp/releases/latest/download/yt-dlp -o yt-dlp && chmod +x yt-dlp"
    },
    "dependencies": {
        "btch-downloader": "^4.1.17",
        "cors": "^2.8.5",
        "express": "^4.21.2",
        "express-rate-limit": "^6.11.2",
        "morgan": "^1.10.1",
        "swagger-ui-express": "^5.0.1",
        "yamljs": "^0.3.0",
        "youtube-dl-exec": "^3.0.22"
    },
    "devDependencies": {
        "nodemon": "^2.0.22"
    }
}